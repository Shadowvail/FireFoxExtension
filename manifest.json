{

  "manifest_version": 2,
  "name": "EbaySearchExtension",
  "version": "0.0.1",

  "description": "Never miss another eBay auction for your custom searches again",

  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "64": "icons/icon-64.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.ebay.com/*"],
      "js": ["content-script.js"]
    }
  ]

}